n = int(input())
a = list(map(int, input().split()))
max_sum = a[0] + a[1]
max_index = (0, 1)

for i in range(1, n-1):  
    cur_sum = a[i] + a[i+1]
    if cur_sum > max_sum:  
      max_sum = cur_sum
      max_index = (i, i+1) 

print(min(a[max_index[0]], a[max_index[1]]), max(a[max_index[0]], a[max_index[1]

 

        

